{
    "name": "鲸鱼阅读",
    "url": "www.jingyu.com",
    "version": 100,
    "search": {
        "url": "http://www.jingyu.com/search/?kw=${key}",
        "list": ".result-list>div",
        "name": ".title > a",
        "author": ".brief > span@match->.*?(?= | )",
        "summary": ".desc",
        "detail": ".title > a"
    },
    "detail": {
        "cover": "a > img",
        "lastChapter": ".details > a",
        "catalog": ".details > div > a:nth-child(3)"
    },
    "catalog": {
        "list": ".volume-list>li",
	      "booklet": {
            "name":"h4",
            "list": " .chapter-list>li"
        },
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#article-content > p"
    }
}
