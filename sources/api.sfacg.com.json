        "cookie": "m.sfacg.com",
        "verify": "https://m.sfacg.com/my/",
        "logged": ".my_menu > a > li@equal->我的书架",
        "vip": "$.data.isVip@equal->true",
        "buy": "$.status.httpCode@equal->200"
    },
