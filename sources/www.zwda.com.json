{
    "name": "E小说",
    "url": "www.zwda.com",
    "version": 104,
    "search": {
        "url": "https://www.zwda.com/search.php?keyword=${key}",
        "list": ".result-list > div",
        "name": "h3",
        "author": "div > p:nth-child(1) > span:nth-child(2)",
        "cover": "img",
        "summary": ".result-game-item-detail > p",
        "detail": "h3 > a"
    },
    "detail": {
        "status": "#info > p:nth-child(3)@match->(?<=·连载)(.+)",
        "update": "#info > p:nth-child(4)@match->(?<=·最后更新)(.+)",
        "lastChapter": "#info > p:nth-child(5)@match->(?<=·最新章节)(.+)"
    },
    "catalog": {
        "list": "#list > dl > dd",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content"
    }
}
